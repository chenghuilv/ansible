    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "ibm_svc_info_module",
            "type": "python",
            "request": "launch",
            "preLaunchTask": "HackingEnvSetup",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}/lib",
            },
            "module": "ansible_collections.ibm.spectrum_virtualize.plugins.modules.ibm_svc_info",
            "args": [
                "${workspaceRoot}/tests/gino/ibm_svc_info.json"
            ]
        },
        {
            "name": "gino_test",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}/lib",
            },
            "module": "ansible.modules.gino.gino_test",
            "args": [
                "${workspaceRoot}/test/gino/args.json"
            ]
        },
    ]
}